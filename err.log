SELECT orders.created, products.name AS product, SUM(items.total) AS total
FROM orders
INNER JOIN items ON orders.order_id=items.order_id
INNER JOIN products ON items.product_id = products.product_id
WHERE DATE(orders.created)=  STR_TO_DATE('2015-05-29', '%Y-%m-%d')
GROUP BY products.name; 


